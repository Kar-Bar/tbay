<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd" >
<generatorConfiguration>
	<properties resource="mybatis/mybatis.properties" />
	<classPathEntry location="${jdbc.jar}" />
	<context id="tennis" targetRuntime="MyBatis3" defaultModelType="flat">
		<!-- 抑制警告 -->
		<property name="suppressTypeWarnings" value="true" />

		<!-- generate entity时，生成hashcode和equals方法 -->
		<!--<plugin type="org.mybatis.generator.plugins.EqualsHashCodePlugin" />-->
		<!-- generate entity时，生成serialVersionUID -->
		<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />
		<!-- 这个插件只会增加字符串字段映射到一个JDBC字符的方法 -->
		<plugin type="org.mybatis.generator.plugins.CaseInsensitiveLikePlugin" />
		<!-- genenat entity时,生成toString -->
		<!--<plugin type="org.mybatis.generator.plugins.ToStringPlugin" />-->

		<!-- 为了防止生成的代码中有很多注释，比较难看，加入下面的配置控制 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="javaFileEncoding" value="UTF-8" />
			<!-- 抑制生成代码的注释 -->
			<property name="suppressAllComments" value="true" />
		</commentGenerator>
		<jdbcConnection driverClass="${jdbc.driver.class}" connectionURL="${jdbc.url}" userId="${jdbc.user}" password="${jdbc.password}" />
		<javaTypeResolver>
			<property name="forceBigDecimals" value="false" />
		</javaTypeResolver>
		<javaModelGenerator targetPackage="${entity.package}" targetProject="${PojoPath}">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
			<!--<property name="rootClass" value="com.caiyi.zlk.tennis.entity.BaseEntity"/>-->
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="${mapper.package}" targetProject="${mapperxmltarget}">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<javaClientGenerator targetPackage="${dao.package}" targetProject="${DaoPath}" type="XMLMAPPER">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<table tableName="tb_item_cat" domainObjectName="ItemCat" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>


		<!--<table tableName="fb_country" domainObjectName="Country" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_league" domainObjectName="League" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_season" domainObjectName="Season" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_league_integral" domainObjectName="LeagueIntegral" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_qualify" domainObjectName="Qualify" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_team" domainObjectName="Team" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_player_new" domainObjectName="PlayerNew" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_player_detail" domainObjectName="PlayerDetail" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		<table tableName="fb_matches" domainObjectName="Matches" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
-->
	</context>
</generatorConfiguration>